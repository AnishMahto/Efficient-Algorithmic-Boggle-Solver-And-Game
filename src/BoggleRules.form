                <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
                  <Font name="Arial" size="17" style="0"/>
                </Property>
                <Property name="text" type="java.lang.String" value="&lt;HTML&gt;Boggle is a word game that takes place on a 4x4 grid of letters. The purpose of the game is maximize your score by trying to make as many words as possible using the letters on the grid. The specific rules are as follows:&lt;HTML&gt;"/>
              </Properties>
            </Component>
            <Component class="javax.swing.JLabel" name="lblRule3">
              <Properties>
                <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
